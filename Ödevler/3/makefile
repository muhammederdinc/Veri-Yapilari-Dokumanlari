Baslat: derle calistir
derle:
	g++ -I ./include -o ./lib/Node.o -c ./src/Node.cpp
	g++ -I ./include -o ./lib/BinarySearchTree.o -c ./src/BinarySearchTree.cpp
	g++ -I ./include -o ./lib/Queue.o -c ./src/Queue.cpp
	g++ -I ./include -o ./lib/Dugum.o -c ./src/Dugum.cpp
	g++ -I ./include -o ./bin/odev ./lib/Node.o ./lib/Queue.o ./lib/BinarySearchTree.o ./lib/Dugum.o ./src/main.cpp
calistir:
	./bin/odev	
	